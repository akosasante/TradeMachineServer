{
  "name": "trade-machine-server",
  "version": "2.0.0",
  "description": "",
  "main": "server.js",
  "scripts": {
    "watch-test": "npm run test -- --watchAll",
    "watch-test-debug": "npm run debug-test -- --watchAll",
    "watch-debug": "concurrently $NODE_DEBUG_OPTION -k -p \"[{name} - {time}]\" -n \"TypeScript,Node\" -c \"cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run serve-debug\"",
    "watch-server": "concurrently -k -p \"[{name} - {time}]\" -n \"TypeScript,Node\" -c \"cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run watch-node\"",
    "build": "npm run build-ts && npm run tslint",
    "serve": "node -r dotenv/config ./dist/server.js",
    "test-debug": "NODE_ENV=test ENABLE_LOGS=true DB_LOGS=true jest --config ./jest.config.js --detectOpenHandles --runInBand --forceExit",
    "test-debug-no-db": "NODE_ENV=test ENABLE_LOGS=true DB_LOGS=false jest --config ./jest.config.js --detectOpenHandles --runInBand --forceExit",
    "test": "ENABLE_LOGS=false NODE_ENV=test jest --config ./jest.ci-config.js --detectOpenHandles --runInBand --silent --bail --forceExit --ci",
    "watch-node": "NODE_ENV=development DEBUG=connect:redis nodemon -- -r dotenv/config ./dist/server.js",
    "build-ts": "tsc",
    "watch-ts": "tsc -w",
    "typecheck": "tsc --noEmit --incremental false",
    "tslint": "tslint -c tslint.json -p tsconfig.json -t stylish",
    "ts-fix": "tslint -c tslint.json -p tsconfig.json -t stylish --fix",
    "debug": "npm run build && npm run watch-debug",
    "serve-debug": "nodemon $NODE_DEBUG_OPTION --inspect dist/server.js",
    "gen-migrate": "npm run build && typeorm migration:generate -f ormconfig -c development -n",
    "gen-migrate-test-empty": "typeorm migration:create -f ormconfig -c test -n empty",
    "gen-migrate-test": "npm run build && typeorm migration:generate -f ormconfig -c test -n",
    "gen-migrate-prod": "npm run build && typeorm migration:generate -f ormconfig -c production -n",
    "run-migrate": "npm run build && typeorm migration:run -f ormconfig -c development",
    "run-migrate-test": "npm run build && typeorm migration:run -f ormconfig -c test",
    "run-migrate-prod": "npm run build && typeorm migration:run -f ormconfig -c production",
    "revert-migrate": "npm run build && typeorm migration:revert -f ormconfig -c development",
    "revert-migrate-test": "npm run build && typeorm migration:revert -f ormconfig -c test",
    "revert-migrate-prod": "npm run build && typeorm migration:revert -f ormconfig -c production"
  },
  "author": "Akosua Asante",
  "license": "ISC",
  "dependencies": {
    "@fast-csv/parse": "^4.3.0",
    "@slack/webhook": "^5.0.3",
    "axios": "^0.18.1",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "bull": "^3.13.0",
    "class-transformer": "^0.2.3",
    "class-validator": "0.10.1",
    "compression": "^1.7.4",
    "connect-redis": "^3.4.2",
    "connect-timeout": "^1.9.0",
    "cors": "^2.8.5",
    "custom-fonts-in-emails": "^4.0.1",
    "date-fns": "^2.8.1",
    "dotenv": "^6.2.0",
    "email-templates": "^7.0.0",
    "express": "^4.17.1",
    "express-session": "^1.17.0",
    "font-awesome-assets": "0.0.8",
    "lodash": "^4.17.15",
    "multer": "^1.4.2",
    "nodemailer": "^6.0.0",
    "nodemailer-sendinblue-transport": "^1.2.3",
    "ordinal": "^1.0.3",
    "pg": "^7.12.1",
    "redis": "^2.8.0",
    "reflect-metadata": "^0.1.13",
    "response-time": "^2.3.2",
    "routing-controllers": "^0.8.0",
    "typedi": "^0.8.0",
    "typeorm": "^0.2.24",
    "uuid": "^3.3.3"
  },
  "devDependencies": {
    "@types/axios": "^0.14",
    "@types/bcryptjs": "^2.4.2",
    "@types/body-parser": "^1.17.1",
    "@types/bull": "^3.12.2",
    "@types/compression": "1.0.1",
    "@types/connect-redis": "^0.0",
    "@types/connect-timeout": "0.0.34",
    "@types/cors": "^2.8.6",
    "@types/dotenv": "^6.1.1",
    "@types/email-templates": "^6.0",
    "@types/express": "^4.17.1",
    "@types/express-session": "^1.15.15",
    "@types/faker": "^4.1.6",
    "@types/jest": "^25.2.1",
    "@types/lodash": "^4.14.144",
    "@types/morgan": "^1.7.37",
    "@types/multer": "^1.3.10",
    "@types/node": "^12.11.2",
    "@types/nodemailer": "^6.2",
    "@types/pg": "^7.11.2",
    "@types/response-time": "^2.3.4",
    "@types/supertest": "^2.0.8",
    "@types/uuid": "^3.4.5",
    "bull-repl": "^0.23.0",
    "faker": "^4.1.0",
    "jest": "^25.3.0",
    "jest-extended": "^0.11.2",
    "morgan": "^1.9.1",
    "nodemon": "^1.19.4",
    "supertest": "^3.4.2",
    "ts-jest": "^25.3.1",
    "ts-node": "^8.4",
    "tslint": "^6.0.0",
    "typescript": "^3.9.6",
    "winston": "^3.2.1"
  }
}
